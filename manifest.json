{
  "manifest_version": 3,
  "name": "Tail Log",
  "description": "This extension scrolls to the end of a webpage.",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "browser_action": {
    "default_icon": "icon.png"
  }
}
